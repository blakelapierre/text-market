tax { @id, insert_at timestamp } {
  income {
    transaction { }
      -> item
      -> tax (tax_amount)
      -> income (income_amount)
      -> info?

    info {
      text text
    }

    item {
      description text
    }

    tax {
      amount bigint
    }

    income {
      amount bigint
    }

    expense {
      amount bigint
    } -> item

    deduction {
      amount bigint
    } -> tax
      -> expense
  }
}